{
  "buildCommand": "npm run build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "images": {
    "domains": ["amris.duckdns.org", "localhost", "example.com", "placehold.co"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "amris.duckdns.org",
        "pathname": "/media/**"
      },
      {
        "protocol": "https",
        "hostname": "amris.duckdns.org",
        "pathname": "/api/**"
      }
    ],
    "sizes": [16, 32, 48, 64, 96, 128, 256, 384]
  }
}
